{
    "name": "JupyterLite",
    "build": {
        "dockerfile": "../Dockerfile",
        "args": {
            "APP_DIR": "/workspaces/wiki3-ai-site"
        }
    },
    // "remoteUser": "root",
    "forwardPorts": [
        8888
    ],
    "portsAttributes": {
        "8888": {
            "label": "JupyterLite",
            "requireLocalPort": true
        }
    },
    "containerEnv": {
        "JUPYTER_PORT": "8888",
        // "GRANT_SUDO": "yes"
        // "RESTARTABLE": "yes"
    }
}
